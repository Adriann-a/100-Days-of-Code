def turn_right():
    turn_left()
    turn_left()
    turn_left()
    
while at_goal() != True:
    if right_is_clear() == True:
        turn_right()
        move()
    elif front_is_clear() == True:
        move()
    elif wall_on_right() == True and wall_in_front() == True:
        turn_left()
        if front_is_clear() == True:
            move()
        else:
            turn_left()
            
